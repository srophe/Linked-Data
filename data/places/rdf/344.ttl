 dcterms:description [
rdfs:label "ܒܰܫܒܳܒܘܬܗ̇ ܕܟܰܪܒܘܬ. ܐܶܬܥܗܰܕ ܫܢܰܬ 1057 ܘܒܗ ܐܶܬܢܰܦܩܘ ܐܰܪܒܥܳܐ ܐܶܦܝܣܩܘܦ̈ܐ. ܒܰܙܘܗܝ ܟܶܢܫܳܐ ܕܡܰܫܠܡ̈ܳܢܐ ܫܢܰܬ 1290 ܘܐܰܚܕܘܗܝ ܒܰܩܛܝܪܳܐ ܚܕܳܪ̈ܰܝ 1131"@syr-Syrj  ;
dcterms:license <http://creativecommons.org/licenses/by/3.0/>  ;];
 dcterms:description [
rdfs:label "بجوار خربوت ذكر سنة 1057 وفيه تخرج اربعة اساقفة نهبته طائفة من المسلمين عام 1290 ثم اغتصبوه حوالي 1311"@ar  ;
dcterms:license <http://creativecommons.org/licenses/by/3.0/>  ;];
 dcterms:description [
rdfs:label "near Kharput, first was mentioned in history in 1057. It produced four bishops. In 1290 it was ransacked by a group of Muslims and Muslims finally occupied it in 1311."@en  ;
dcterms:license <http://creativecommons.org/licenses/by/3.0/>  ;];
